-- Crear propietario
insert:
INSERT INTO PropietarioTable (dni, nombre, apellidos, correo, telefono) VALUES (?,?, ?, ?, ?);

-- Actualizar propietario
update:
UPDATE PropietarioTable SET dni= ?, nombre = ?, apellidos = ?, correo = ?, telefono = ? WHERE dni = ?;

-- Borrar por ID propietario
deleteById:
DELETE FROM PropietarioTable WHERE dni = ?;

-- Buscar por ID propietario
selectById:
SELECT * FROM PropietarioTable WHERE dni = ?;

