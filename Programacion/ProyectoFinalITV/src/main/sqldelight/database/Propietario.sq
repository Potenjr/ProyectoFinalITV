CREATE TABLE IF NOT EXISTS PropietarioTable
(
    dni       TEXT  PRIMARY KEY,
    nombre    TEXT  NOT NULL,
    apellidos TEXT  NOT NULL,
    correo    TEXT  NOT NULL,
    telefono  TEXT  NOT NULL
);

